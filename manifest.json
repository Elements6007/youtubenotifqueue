{
    "name": "Notifications Queue",
    "version": "0.0.1",
    "manifest_version": 3,
    "host_permissions": ["https://*.youtube.com/*"],
    "background": {
        "service_worker": "background.js"
      },
    "description" : "Add YouTube notifications to queue",
    "content_scripts" : [
       {
        "matches": ["https://*.youtube.com/*"],
        "js" : ["content_script.js"]
       }
    ]
}
